<div class="save-filter-container">
  <div class="panel panel-default">
    <div class="panel-heading">
      <button type="button" class="close pull-right" aria-hidden="true">&times;</button>
      <h3 class="panel-title">Save Filter</h3>
    </div>
    <div class="clearfix">&nbsp;</div>
    <div class="panel-body">
      <span><strong>Name Your Filter: </strong></span>
      <br/>
      <input type="text" name="name" id='filter-name'/>
      <br/>
      <br/>
      <button type="button" class="btn btn-primary" id="submit-save-filter">Save</button>
      <span>&nbsp;</span>
      <button type="button" class="btn btn-default" id="cancel-save-filter">Cancel</button>
    </div>
  </div>
</div>
